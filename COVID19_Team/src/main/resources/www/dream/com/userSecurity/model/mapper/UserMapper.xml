<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="www.dream.com.userSecurity.model.mapper.UserMapper">
	<resultMap id="UserResultMap"
		type="www.dream.com.userSecurity.model.UserVO">
		<result property="partyId" column="PARTY_ID" />
		<result property="userNum" column="user_num" />
		<result property="userId" column="user_id" />
		<result property="password" column="PASSWORD" />
		<result property="email" column="EMAIL" />
		<result property="authority" column="AUTHORITY" />
		<result property="enabled" column="ENABLED" />
	</resultMap>

	<insert id="signUser">
		<![CDATA[
		insert into T_USER(PARTY_ID, user_num, EMAIL, user_id, PASSWORD)
		       values(3, seq4userid.nextval, #{email, jdbcType=VARCHAR}, #{userId, jdbcType=VARCHAR}, #{password, jdbcType=VARCHAR})
		]]>
	</insert>





</mapper>